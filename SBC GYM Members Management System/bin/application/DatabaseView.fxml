<FlowPane style="-fx-background-color: linear-gradient(to right top, #051937, #004d7a, #008793, #00bf72, #a8eb12);" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DatabaseViewController">
   <children>
      <AnchorPane prefHeight="629.0" prefWidth="868.0" style="-fx-background-color: linear-gradient(to right top, #051937, #004d7a, #008793, #00bf72, #a8eb12);">
         <children>
            <ListView fx:id="membersListView" layoutY="55.0" prefHeight="506.0" prefWidth="868.0" style="-fx-background-color: linear-gradient(to right top, #051937, #004d7a, #008793, #00bf72, #a8eb12);" />
            <Label alignment="CENTER" layoutX="276.0" layoutY="14.0" prefHeight="36.0" prefWidth="318.0" text="Registered Memebers List" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Label>
            <HBox layoutX="132.0" layoutY="572.0" prefHeight="43.0" prefWidth="605.0">
               <children>
                  <Button fx:id="viewButton" mnemonicParsing="false" onAction="#handleViewButtonClick" prefHeight="43.0" prefWidth="105.0" text="View">
                     <HBox.margin>
                        <Insets left="36.5" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="editButton" mnemonicParsing="false" onAction="#handleEditButtonClick" prefHeight="43.0" prefWidth="105.0" text="Edit">
                     <HBox.margin>
                        <Insets left="36.5" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#handleDeleteButtonClick" prefHeight="43.0" prefWidth="105.0" text="Delete">
                     <HBox.margin>
                        <Insets left="36.5" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBackButtonClick" prefHeight="43.0" prefWidth="105.0" text="Back">
                     <HBox.margin>
                        <Insets left="36.5" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</FlowPane>
